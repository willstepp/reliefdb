<h1>Listing faq_categories</h1>

<table>
  <tr>
  <% for column in FaqCategory.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for faq_category in @faq_categories %>
  <tr>
  <% for column in FaqCategory.content_columns %>
    <td><%=h faq_category.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => faq_category %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => faq_category %></td>
    <td><%= link_to 'Destroy', {:action => 'destroy', :id => faq_category}, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @faq_category_pages.current.previous } if @faq_category_pages.current.previous %>
<%= link_to 'Next page', { :page => @faq_category_pages.current.next } if @faq_category_pages.current.next %> 

<br />

<%= link_to 'New faq_category', :action => 'new' %>
